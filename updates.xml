<?xml version="1.0" encoding="UTF-8"?>
<manifest>

  <remote name="Gitlab" fetch="https://gitlab.com" />

  <!-- Static OTA updater resource 
  <project path="OTA" name="lin16-microG/OTA" revision="changelog" />
  -->

  <!-- GrapheneOS' hardened_malloc (forked from GlassROM) -->
  <project path="external/hardened_malloc" name="lin17-microG/hardened_malloc" revision="master" />

  <!-- microG Build, prebuilt apks -->
  <project path="prebuilts/prebuiltapks" name="MSe1969/android_prebuilts_prebuiltapks" revision="master" />

  <!-- Bromite SystemWebView & Browser prebuilt-->
  <project path="external/Bromite" name="MSe1969/android_external_Bromite" remote="Gitlab" revision="master" />
  <project path="external/bromite-webview" name="MSe1969/android_external_bromite-webview" revision="master" />

  <!-- AuroraStore prebuilt-->
  <project path="prebuilts/AuroraStore" name="MSe1969/AuroraStore-prebuilt" revision="master" />

  <!-- Patches & root-scripts Directory -->
  <project path="z_patches" name="lin16-microG/z_patches" />
  <project path="z_patches/root-scripts" name="lin16-microG/root-scripts" revision="master"/>

  <!-- Replaced/enhanced Repositories -->
  <remove-project name="LineageOS/android_build" />
  <project path="build/make" name="lin17-microG/android_build" groups="pdk" >
    <copyfile src="core/root.mk" dest="Makefile" />
    <linkfile src="CleanSpec.mk" dest="build/CleanSpec.mk" />
    <linkfile src="buildspec.mk.default" dest="build/buildspec.mk.default" />
    <linkfile src="core" dest="build/core" />
    <linkfile src="envsetup.sh" dest="build/envsetup.sh" />
    <linkfile src="target" dest="build/target" />
    <linkfile src="tools" dest="build/tools" />
  </project>
  <remove-project name="LineageOS/android_art" />
  <project path="art" name="lin17-microG/android_art" />
  <remove-project name="LineageOS/android_frameworks_base" />
  <project path="frameworks/base" name="lin17-microG/android_frameworks_base" />
  <remove-project name="LineageOS/android_frameworks_native" />
  <project path="frameworks/native" name="lin17-microG/android_frameworks_native" />
  <remove-project name="platform/libcore" />
  <project path="libcore" name="lin17-microG/android_libcore" />
  <remove-project name="LineageOS/android_packages_apps_LineageParts" />
  <project path="packages/apps/LineageParts" name="lin17-microG/android_packages_apps_LineageParts" />
  <remove-project name="LineageOS/android_packages_apps_Nfc" />
  <project path="packages/apps/Nfc" name="lin17-microG/android_packages_apps_Nfc" />
  <remove-project name="LineageOS/android_packages_apps_Settings" />
  <project path="packages/apps/Settings" name="lin17-microG/android_packages_apps_Settings" />
  <remove-project name="LineageOS/android_system_core" />
  <project path="system/core" name="lin17-microG/android_system_core" />
  <remove-project name="LineageOS/android_system_sepolicy" />
  <project path="system/sepolicy" name="lin17-microG/android_system_sepolicy" />
  <remove-project name="LineageOS/android_vendor_lineage" />
  <project path="vendor/lineage" name="lin17-microG/android_vendor_lineage" />

</manifest>
